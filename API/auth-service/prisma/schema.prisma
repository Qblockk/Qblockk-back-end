generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model app_users {
  id         BigInt    @id @default(autoincrement())
  email      String    @unique
  password   String
  full_name  String
  phone      String?   @map("Phone")
  role       String    @default("user")
  last_log   DateTime? @map("Last_log")
  created_at DateTime  @default(now())

  @@map("app_users")
}