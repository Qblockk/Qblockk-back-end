generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model app_users {
  id         BigInt   @id @default(autoincrement())
  email      String   @unique
  password   String
  full_name  String
  role       String   @default("user")
  created_at DateTime @default(now())

  @@map("app_users")
}
